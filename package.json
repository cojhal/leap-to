{
  "name": "leap-to",
  "version": "0.2.0",
  "description": "CLI for cd'ing to path aliases",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "spago test && purty validate src && purty validate test",
    "prepare": "spago bundle-app --platform node --minify",
    "format": "purty --write src && purty --write test"
  },
  "bin": {
    "leap": "bin.js"
  },
  "files": [
    "bin.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cojhal/leap-to.git"
  },
  "author": "Connor Hall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cojhal/leap-to/issues"
  },
  "homepage": "https://github.com/cojhal/leap-to#readme",
  "devDependencies": {
    "esbuild": "^0.15.2",
    "purescript": "^0.15.0",
    "purty": "^7.0.0",
    "spago": "^0.20.8"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
